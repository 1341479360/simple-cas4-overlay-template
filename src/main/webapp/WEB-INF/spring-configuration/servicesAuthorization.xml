<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="serviceAuthorizationAction" class="net.unicon.cas.addon.registeredservices.authorization.webflow.ServiceAuthorizationAction"
          c:servicesManager-ref="servicesManager"
          c:ticketRegistry-ref="ticketRegistry"
          c:registeredServiceAuthorizer-ref="registeredServiceAuthorizer"/>

    <bean id="registeredServiceAuthorizer" class="net.unicon.cas.addon.registeredservices.authorization.DefaultRegisteredServiceAuthorizer"/>

</beans>